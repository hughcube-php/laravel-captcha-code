{
    "name": "hughcube/captcha-code",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "hugh.li",
            "email": "hugh.li@foxmail.com"
        }
    ],
    "require": {
        "php": "7.*|8.*",
        "illuminate/support": "5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "laravel/framework": "^8.0",
        "laravel/lumen-framework": "^v8.0",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^0.12.84",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "HughCube\\Laravel\\CaptchaCode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HughCube\\Laravel\\CaptchaCode\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HughCube\\Laravel\\CaptchaCode\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --verbose --coverage-clover=coverage.clover",
        "phpstan": "phpstan analyse -vvv",
        "check-style": "phpcs -p --standard=PSR2 src/ -v",
        "fix-style": "phpcbf -p --standard=PSR2 src/ -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
