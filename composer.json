{
    "name": "hughcube/laravel-captcha-code",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "hugh.li",
            "email": "hugh.li@foxmail.com"
        }
    ],
    "require": {
        "php": "7.*|8.*",
        "hughcube/laravel-service-support": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "laravel/framework": "*",
        "orchestra/testbench": "*",
        "laravel/lumen-framework": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "HughCube\\Laravel\\CaptchaCode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HughCube\\Laravel\\CaptchaCode\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse -vvv",
        "check-style": "phpcs -p --standard=PSR2 src/ -v",
        "fix-style": "phpcbf -p --standard=PSR2 src/ -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
